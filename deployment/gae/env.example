# Example Environment File for AOLF GSEC
# Copy this file to .env.dev, .env.uat, or .env.prod and fill in the values

# Database Configuration
POSTGRES_USER=your_db_username
POSTGRES_PASSWORD=your_db_password
POSTGRES_DB=your_db_name
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# JWT Configuration
JWT_SECRET_KEY=your_jwt_secret_key

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Google Maps API
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# SendGrid Email Configuration
SENDGRID_API_KEY=your_sendgrid_api_key
FROM_EMAIL=your_sender_email@example.com
ENABLE_EMAIL=true

# Frontend Configuration
REACT_APP_API_URL=https://your-api-url.appspot.com
REACT_APP_ENVIRONMENT=dev
REACT_APP_API_BASE_URL=https://your-api-url.appspot.com

# AWS Configuration (if applicable)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=your_aws_region 